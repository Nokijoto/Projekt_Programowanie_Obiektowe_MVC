@model Projekt_MVC.Models.TDriveModel.EditTestDriveModel
@{
}
<form method="post" asp-controller="TestDrive" asp-action="EditTestDriveDetails">

    <div class="form-group">
        <label for="id">ID</label>
        <input type="text" class="form-control" id="id" name="id" placeholder="ID" readonly value="@Model.ID">
    </div>
    <div class="form-group">
        <label for="imie">Imie</label>
        <input type="text" class="form-control" id="imie" name="imie" placeholder="placeholder" value="@Model.Imie">
    </div>
    <div class="form-group">
        <label for="nazwisko">Nazwisko</label>
        <input type="text" class="form-control" id="nazwisko" name="nazwisko" placeholder="placeholder" value="@Model.Nazwisko">
    </div>
    <div class="form-group">
        <label for="data">Data</label>
        <input type="text" class="form-control" id="data" name="data" placeholder="placeholder" value="@Model.Data">
    </div>
    <div class="form-group">
        <label for="pesel">Pesel</label>
        <input type="text" class="form-control" id="pesel" name="pesel" placeholder="placeholder" value="@Model.Pesel">
    </div>
    <div class="form-group">
        <label for="nrtel">NrTel</label>
        <input type="number" class="form-control" id="nrtel" name="nrtel" placeholder="placeholder" value="@Model.NrTel">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>